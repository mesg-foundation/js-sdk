    "@types/listr": "^0.14.2",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.pick": "^4.4.6",
    "@types/long": "^4.0.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.18",
    "@types/node-fetch": "^2.5.4",
    "@types/rimraf": "^2.0.2",
    "@types/tar": "^4.0.3",
    "@types/uuid": "^3.4.5",
