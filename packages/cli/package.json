  "license": "ISC",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "mesg-cli",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ],
    "topics": {
      "service": {
        "description": "Manage services"
      },
      "process": {
        "description": "Manage processes"
      }
    }
  }
