{
  "name": "@mesg/cli",
  "description": "MESG CLI",
  "version": "0.3.0",
  "author": "Anthony ESTEBE @antho1404",
  "main": "lib/index",
  "types": "lib/index",
  "bin": {
    "mesg-cli": "./bin/run"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
